    email = javugi@gmail.com
[github]
    user = JaviLorbada
    username = JaviLorbada
[core] # Use custom `.gitignore` and `.gitattributes`
    excludesfile =  ~/.gitignore_global 
    excludesfile = ~/.gitignore
    attributesfile = ~/.gitattributes
    autocrlf = false
    safecrlf = false
    quotepath = false
[difftool "sourcetree"]
    cmd = opendiff \"$LOCAL\" \"$REMOTE\"
    path = 
[mergetool "sourcetree"]
    cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
    trustExitCode = true
[color]
    branch = auto
    diff = auto
    status = auto
    ui = true
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[merge] # Include summaries of merged commits in newly created merge commit messages
    log = true
[alias]
    a = add .
    co = checkout
    cm = commit -m
    ca = commit -a
    ci = commit -am
    st = status
    s = status -sb
    br = branch
    brd = branch -d
    ss = stash save
    sp = stash pop
    us = submodule update --recursive
    p = push
    pu = push -u
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    hist2 = log --graph --pretty='%C(yellow)%h (%cr) |%Cred %an%C(white)%d%Creset %s'
    type = cat-file -t
    dump = cat-file -p
    mf = merge --no-ff
[push]
    default = simple
[diff "localizablestrings"]
    textconv = "iconv -f utf-16 -t utf-8"
